{
  "name": "console-vue-query-devtools",
  "version": "0.0.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev:sdk": "pnpm --filter console-vue-query-devtools-sdk dev",
    "build:sdk": "pnpm --filter console-vue-query-devtools-sdk build",
    "dev:extension": "pnpm --filter @console-vue-query-devtools/extension dev",
    "build:extension": "pnpm --filter @console-vue-query-devtools/extension build",
    "build": "pnpm build:sdk && pnpm build:extension",
    "typecheck": "pnpm -r run typecheck"
  },
  "packageManager": "pnpm@8.14.0",
  "devDependencies": {
    "@tanstack/vue-query": "^5.74.7",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "^9.21.1",
    "prettier": "^3.2.5",
    "typescript": "^5.8.3",
    "vite": "^6.3.3"
  }
}
